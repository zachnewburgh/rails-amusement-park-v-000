<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<%= @user.name %>
Mood: <%= @user.try(:mood) %>
Height: <%= @user.try(:height) %>
Tickets: <%= @user.try(:tickets) %>
Admin: <%= @user.try(:admin) == true ? "ADMIN" : "USER" %>

<%= link_to "See attractions", attractions_path %>
<%= link_to "Log Out", signout_path, method: :delete %>
